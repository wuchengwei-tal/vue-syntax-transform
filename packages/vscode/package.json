{
  "publisher": "wcw",
  "name": "vue-transform",
  "displayName": "Vue Transform",
  "description": "Vue Syntax Transform",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/",
    "directory": "packages/vscode"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "vue-transform.reactivityTransform",
        "title": "Vue Reactivity Transform"
      },
      {
        "command": "vue-transform.v2ToV3Setup",
        "title": "Vue2 to Vue3 Setup Syntax"
      }
    ]
  },
  "scripts": {
    "vsix": "vsce package",
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsup",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "@vscode/vsce": "2.17.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  }
}