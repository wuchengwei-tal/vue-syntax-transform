{
  "publisher": "wcw",
  "name": "vue-transform",
  "displayName": "Vue Transform",
  "description": "Vue Syntax Transform",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/",
    "directory": "packages/vscode"
  },
  "engines": {
    "vscode": "^1.78.1"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "vue-transform.reactivityTransform",
        "title": "[Vue Syntax Transform] Reactivity Transform"
      },
      {
        "command": "vue-transform.v2ToV3Setup",
        "title": "[Vue Syntax Transform] Vue2 to Vue3 Setup"
      },
      {
        "command": "vue-transform.v3SetupToV2",
        "title": "[Vue Syntax Transform] Vue3 Setup to Vue2"
      }
    ]
  },
  "scripts": {
    "vsix": "vsce package",
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsup",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.78.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vscode/test-electron": "^2.3.2",
    "@vscode/vsce": "2.19.0",
    "eslint": "^8.41.0",
    "glob": "^10.2.6",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  }
}